	<queryString>
		<![CDATA[SELECT
     *,
     vendas.`data_venda` AS vendas_data_venda,
     vendas.`id` AS vendas_id,
     vendas.`desconto` AS vendas_desconto,
     produtos.`nome` AS produtos_nome,
     vendas_produtos.`qtd` AS vendas_produtos_qtd,
     vendedores.`nome` AS vendedores_nome
FROM
     `vendas` vendas INNER JOIN `vendas_produtos` vendas_produtos ON vendas.`id` = vendas_produtos.`id_venda`
     INNER JOIN `produtos` produtos ON vendas_produtos.`id_produto` = produtos.`id`
     INNER JOIN `vendedores` vendedores ON vendas.`id_vendedor` = vendedores.`id` where data_venda >= $P{data_inicial} && data_venda <= $P{data_final};]]>
	</queryString>
